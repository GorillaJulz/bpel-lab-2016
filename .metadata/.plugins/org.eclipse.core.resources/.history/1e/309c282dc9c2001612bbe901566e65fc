                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns:amount]]></bpel:query>
                </bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="payload" variable="input">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:input/tns:orderID]]></bpel:query>
                </bpel:from>
                <bpel:to part="conductPaymentInput" variable="PaymentServicePLRequest1">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns:orderId]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        <bpel:invoke name="Payment" partnerLink="PaymentServicePL" operation="conductPayment" portType="ns:PaymentPortType" inputVariable="PaymentServicePLRequest1" outputVariable="PaymentServicePLResponse1"></bpel:invoke>
        <bpel:if name="IfPaymentUnsuccessful">
            <bpel:condition><![CDATA[$PaymentServicePLResponse1.conductPaymentOutput/ns:status != "payed"]]></bpel:condition>
            <bpel:scope name="Scope">
                <bpel:sequence name="Sequence1">
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    
                    <bpel:invoke name="cancelOrder" partnerLink="OrderServicePL" operation="cancelOrder" portType="ns:OrderPortType" inputVariable="OrderServicePLRequest3"></bpel:invoke>
                    <bpel:exit name="Exit1"></bpel:exit>
                </bpel:sequence>
                <bpel:variables>
                    <bpel:variable name="OrderServicePLRequest3" messageType="ns:cancelOrderInput"></bpel:variable>
                </bpel:variables>
            </bpel:scope>
        </bpel:if>
        
        <bpel:invoke name="MakeCustomerHappy" partnerLink="ShipmentServicePL" operation="shipProducts" portType="ns:ShipmentPortType" inputVariable="ShipmentServicePLRequest1" outputVariable="ShipmentServicePLResponse1"></bpel:invoke>
        <bpel:assign validate="no" name="AssignDetails">
            
            
            <bpel:copy>
                <bpel:from><bpel:literal><tns:conductorderResponse xmlns:tns="julz.babis" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <tns:result>tns:result</tns:result>
</tns:conductorderResponse>
</bpel:literal></bpel:from>
                <bpel:to variable="output" part="payload"></bpel:to>
            </bpel:copy>
            <bpel:copy>
                <bpel:from part="shipProductsOutput" variable="ShipmentServicePLResponse1">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[ns:status]]></bpel:query>
                </bpel:from>
                <bpel:to part="payload" variable="output">
                    <bpel:query queryLanguage="urn:oasis:names:tc:wsbpel:2.0:sublang:xpath1.0"><![CDATA[tns:result]]></bpel:query>
                </bpel:to>
            </bpel:copy>
        </bpel:assign>
        
        <bpel:reply name="replyOutput" partnerLink="client" portType="tns:conductorder" operation="process" variable="output" />
    
